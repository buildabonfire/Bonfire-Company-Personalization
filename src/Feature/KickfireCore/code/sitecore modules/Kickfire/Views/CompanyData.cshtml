                <span class="memberDetailsValue">@Model.LinkedIn</span>
            </div>
            <div class="section__company">
                <span class="memberDetailsLabel">IsIsp:</span>
                <span class="memberDetailsValue">@Model.IsIsp</span>
            </div>
            <div class="section__company">
                <span class="memberDetailsLabel">Confidence:</span>
                <span class="memberDetailsValue">@Model.Confidence</span>
            </div>
        </div>
    </div>

}